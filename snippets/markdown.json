{
    "Links": {
        "prefix": ["l", "link"],
        "body": ["[${1:text}](${2:url}) ${0}"],
    },
    "URLS": {
        "prefix": ["u", "url"],
        "body": ["<${1}> ${0}"],
    },
    "Images": {
        "prefix": "img",
        "body": ["![${1:alt text}](${2:path}) ${0}"],
    },
    "Insert strikethrough": {
        "prefix": "strikethrough",
        "body": "~~${1}~~ ${0}",
    },
    "Insert bold text": {
        "prefix": ["bold", "b"],
        "body": "**${1}** $0",
    },
    "Insert italic text": {
        "prefix": ["i", "italic"],
        "body": "*${1}* $0",
    },
    "Insert bold and italic text": {
        "prefix": ["bold and italic", "bi"],
        "body": "***${1}*** $0",
    },
    "Insert quoted text": {
        "prefix": "quote",
        "body": "> ${1}",
    },
    "Insert code": {
        "prefix": "code",
        "body": "`${1}` $0",
    },
    "Insert code block": {
        "prefix": "codeblock",
        "body": ["```${1:language}", "$0", "```"],
    },
    "Insert unordered list": {
        "prefix": "unordered list",
        "body": ["- ${1:first}", "- ${2:second}", "- ${3:third}", "$0"],
    },
    "Insert ordered list": {
        "prefix": "ordered list",
        "body": ["1. ${1:first}", "2. ${2:second}", "3. ${3:third}", "$0"],
    },
    "Insert horizontal rule": {
        "prefix": "horizontal rule",
        "body": "----------\n",
    },
    "Task list": {
        "prefix": "task",
        "body": ["- [${1| ,x|}] ${2:text}", "${0}"],
    },
    "Table": {
        "prefix": "table",
        "body": [
            "| ${1:Column 1} | ${2:Column 2}  | ${3:Column 3} |",
            "| :------- | :-------: | -------: |",
            "| ${4:Item1}    | ${5:Item1}     | ${6:Item1}    |",
            "${0}"
        ],
    },
    "Insert subscript": {
        "prefix": "sub",
        "body": ["${1}<sub>${0}"],
    },
    "Insert superscript": {
        "prefix": "sup",
        "body": ["${1}<sup>${0}"],
    },
    "Insert generic callout": { 
        "prefix": "callout",
        "body": "> [!${1:note}]\n> ",
    },
    "Insert Note": {
        "prefix": ["note", "n"],
        "body": "> [!NOTE]\n> ",
    },
    "Insert Tip": {
        "prefix": ["tip", "t"],
        "body": "> [!TIP]\n> ",
    },
    "Insert Important": {
        "prefix": ["important", "imp"],
        "body": "> [!IMPORTANT]\n> ",
    },
    "Insert Warning": {
        "prefix": ["warning", "w"],
        "body": "> [!WARNING]\n> ",
    },
    "Insert Caution": {
        "prefix": ["caution", "c"],
        "body": "> [!CAUTION]\n> ",
    }
}
